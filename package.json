{
    "name": "jolimoi-challenge",
    "version": "1.0.0",
    "description": "jolimoi challenge with the number to roman convertion",
    "scripts": {
      "start": "nodemon ./server.js --ignore public/ --exec babel-node"
    },
    "keywords": [
      "Node.js",
      "Javascript",
      "Express"
    ],
    "author": "Ange Parfait (a.k.a Sanix darker)",
    "dependencies": {
      "body-parser": "^1.15.2",
      "cors": "^2.8.0",
      "dotenv": "^6.0.0",
      "express": "4.14.0",
      "lodash": "^4.15.0",
      "moment": "^2.22.2"
    },
    "devDependencies": {
      "apidoc": "^0.17.6",
      "babel-cli": "6.16.0",
      "babel-core": "^6.17.0",
      "babel-eslint": "^7.0.0",
      "babel-plugin-inline-import": "^2.0.1",
      "babel-plugin-transform-async-to-module-method": "^6.24.1",
      "babel-plugin-transform-object-rest-spread": "^6.26.0",
      "babel-plugin-transform-runtime": "^6.23.0",
      "babel-polyfill": "^6.16.0",
      "babel-preset-env": "^1.7.0",
      "babel-preset-es2015": "^6.16.0",
      "babel-preset-react": "^6.5.0",
      "babel-preset-stage-0": "^6.16.0",
      "babel-runtime": "^6.26.0",
      "eslint": "^3.19.0",
      "eslint-config-airbnb": "^12.0.0",
      "eslint-plugin-import": "^2.0.1",
      "eslint-plugin-react": "^6.4.1",
      "nodemon": "^1.9.1"
    },
    "eslintConfig": {
      "parser": "babel-eslint",
      "extends": [
        "airbnb/base",
        "plugin:import/errors"
      ],
      "rules": {
        "no-use-before-define": 0,
        "arrow-body-style": 0,
        "dot-notation": 0,
        "no-console": 0,
        "new-cap": 0,
        "no-trailing-spaces": 0,
        "no-param-reassign": 0
      },
      "env": {
        "mocha": true
      }
    },
    "apidoc": {
      "version": "1.0.0",
      "defaultVersion": "1.0.0",
      "name": "Dispo X API",
      "title": "Dispo X API",
      "url": "https://api.dispox.com/v1",
      "header": {
        "title": "Overview",
        "filename": "public/api/apiheader.md"
      }
    },
    "babel": {
      "presets": [
        "env"
      ]
    }
}